<category id="morpxSentry" name="Sentry" colour="#EF5411">
  <category id="morpxSentry1" name="Sentry1" colour="#EF5411">
    <category id="morpxSentry1_Setup" name="{$CATEGORY_MORPX_SENTRY_SETUP}" colour="#EF5411">
      <!-- 初始化 Sentry1 -->
      <block type="Sentry1Begin"></block>
      <!-- 恢复默认设置 -->
      <!-- <block type="Sentry1SetDefault"></block> -->
      <!-- 启用算法 -->
      <block type="Sentry1VisionSetStatus">
        <value name="VisionType">
          <shadow type="Sentry1VisionType"></shadow>
        </value>
      </block>
      <!-- 设置 LED -->
      <block type="Sentry1LedSetColor">
        <value name="level">
          <shadow type="math_number">
            <field name="NUM">1</field>
          </shadow>
        </value>
      </block>
      <!-- 设置 算法参数个数-->
      <block type="Sentry1SetParamNum">
        <value name="VisionType">
          <shadow type="Sentry1VisionType"></shadow>
        </value>
        <value name="max_num">
          <shadow type="math_number">
            <field name="NUM">1</field>
          </shadow>
        </value>
      </block>
      <!-- Color 算法参数-->
      <block type="Sentry1SetColorParam">
        <value name="x">
          <shadow type="math_number">
            <field name="NUM">120</field>
          </shadow>
        </value>
        <value name="y">
          <shadow type="math_number">
            <field name="NUM">120</field>
          </shadow>
        </value>
        <value name="w">
          <shadow type="math_number">
            <field name="NUM">20</field>
          </shadow>
        </value>
        <value name="h">
          <shadow type="math_number">
            <field name="NUM">20</field>
          </shadow>
        </value>
        <value name="objid">
          <shadow type="math_number">
            <field name="NUM">1</field>
          </shadow>
        </value>
      </block>
      <!-- Blod 算法参数-->
      <block type="Sentry1SetBlodParam">
        <value name="w">
          <shadow type="math_number">
            <field name="NUM">15</field>
          </shadow>
        </value>
        <value name="h">
          <shadow type="math_number">
            <field name="NUM">15</field>
          </shadow>
        </value>
        <value name="objid">
          <shadow type="math_number">
            <field name="NUM">1</field>
          </shadow>
        </value>
      </block>
      <!-- <block type="Sentry1CameraSetZoom"></block>
      <block type="Sentry1UartSetBaudrate"></block>
      <block type="Sentry1CameraSetAwb"></block>
      <block type="Sentry1CameraSetFPS"></block> -->
    </category>
    <category id="morpxSentry1_Run" name="{$CATEGORY_MORPX_SENTRY_RUN}" colour="#EAA20A">
      <block type="Sentry1Detected">
        <value name="VisionType">
          <shadow type="Sentry1VisionType"></shadow>
        </value>
      </block>
      <block type="Sentry1GetValue">
        <value name="VisionType">
          <shadow type="Sentry1VisionType"></shadow>
        </value>
        <value name="objid">
          <shadow type="math_number">
            <field name="NUM">1</field>
          </shadow>
        </value>
      </block>
      <block type="Sentry1ColorRcgValue">
        <value name="objid">
          <shadow type="math_number">
            <field name="NUM">1</field>
          </shadow>
        </value>
      </block>
      <block type="Sentry1LineValue">
        <value name="objid">
          <shadow type="math_number">
            <field name="NUM">1</field>
          </shadow>
        </value>
      </block>
      <block type="Sentry1DetectedColor">
        <value name="objid">
          <shadow type="math_number">
            <field name="NUM">1</field>
          </shadow>
        </value>
      </block>
      <block type="Sentry1DetectedBlob">
        <value name="objid">
          <shadow type="math_number">
            <field name="NUM">1</field>
          </shadow>
        </value>
      </block>
      <block type="Sentry1DetectedCard">
        <value name="objid">
          <shadow type="math_number">
            <field name="NUM">1</field>
          </shadow>
        </value>
      </block>
      <!-- <block type="Sentry1Rows"></block>
      <block type="Sentry1Cols"></block> -->
    </category>
  </category>
