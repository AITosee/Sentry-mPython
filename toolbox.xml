<category id="morpxSentry" name="Sentry" colour="#EF5411">
  <category id="morpxSentry_Setup" name="{$CATEGORY_MORPX_SENTRY_SETUP}" colour="#EF5411">
    <!-- 初始化 Sentry -->
    <block type="SentryBegin"></block>

    <!-- 恢复默认设置 -->
    <block type="SentrySetDefault"></block>

    <!-- 启用算法 -->
    <block type="SentryVisionSetStatus">
      <value name="VisionType">
        <shadow type="Vs2VisionType"></shadow>
      </value>
    </block>

    <!-- 设置 算法参数个数-->
    <block type="SentrySetParamNum">
      <value name="VisionType">
        <shadow type="Vs2VisionType"></shadow>
      </value>
      <value name="max_num">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>

    <!-- 设置 LCD-->
    <block type="SentryLcdSetMode">
      <value name="mode">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>

    <!-- 设置 LED -->
    <block type="SentryLedSetColor">
      <value name="level">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>

    <block type="SentryCameraSetZoom"></block>

    <block type="SentryCameraSetRotate"></block>

    <block type="SentryCameraSetAwb"></block>

    <block type="SentryCameraSetFPS"></block>

    <block type="SentryCameraSetBrightness"></block>

    <block type="SentryCameraSetContrast"></block>

    <block type="SentryCameraSetSaturation"></block>

    <block type="SentryCameraSetShaprness"></block>

  </category>

  <category id="morpxSentry_Run" name="{$CATEGORY_MORPX_SENTRY_RUN}" colour="#EAA20A">

    <block type="SentryDetected">
       <value name="VisionType">
        <shadow type="Vs2VisionType"></shadow>
      </value>
    </block>

    <block type="SentryGetValue">
       <value name="VisionType">
        <shadow type="Vs2VisionType"></shadow>
      </value>
      <value name="objid">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>   
    </block>

    <block type="SentryColorRcgValue"></block>

    <block type="SentryQrRcgValue"></block>

    <block type="SentryGetQrCodeValue"></block>

    <block type="SentryRows"></block>

    <block type="SentryCols"></block>

    <block type="SentryDetectedColor"></block>

    <block type="SentryDetectedBlob"></block>

    <block type="SentryDetectedCard"></block>

    <block type="SentryDetected20Class"></block>

  </category>
</category>
